{
  "name": "@peterting/psd-plugin",
  "version": "1.0.0",
  "description": "A simple psdjs plugin",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
	"test": "jest --config jestconfig.json",
	"prepare": "npm run build",
	"prepublishOnly": "npm test && npm run lint",
	"preversion": "npm run lint",
	"version": "npm run format && git add -A src",
	"postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeterTing/psd-plugins.git"
  },
  "keywords": ["psd", "js"],
  "author": "PeterTing",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PeterTing/psd-plugins/issues"
  },
  "homepage": "https://github.com/PeterTing/psd-plugins#readme",
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/node": "^12.7.4",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {}
}
